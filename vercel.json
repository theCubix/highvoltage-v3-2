{
  "routes": [
    {
      "src": "/events",
      "dest": "/konzerte"
    },
    {
      "src": "/gallery",
      "dest": "/galerie"
    },
    {
      "src": "/video",
      "status": 308,
      "headers": { "Location": "https://www.youtube.com/watch?v=QjkTEP7yo1A" }
    },
    {
      "src": "/fonts/(.*)",
      "headers": { "Cache-Control": "max-age=31536000" },
      "dest": "/fonts/$1"
    }
  ]
}